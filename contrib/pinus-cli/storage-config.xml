<?xml version="1.0" encoding="UTF-8" ?>
<storage-config>

	<!-- text hash algo -->
	<db.cluster.hash.algo>bernstein</db.cluster.hash.algo>
	<!-- zookeeper connection -->
	<db.cluster.zk>192.168.4.71:2181</db.cluster.zk>
	<!-- id generator batch -->
	<db.cluster.generateid.batch>1
	</db.cluster.generateid.batch>

	<db-connection-pool>
		<maxActive>30</maxActive>
		<minIdle>1</minIdle>
		<maxIdle>5</maxIdle>
		<initialSize>10</initialSize>
		<removeAbandoned>true
		</removeAbandoned>
		<removeAbandonedTimeout>10
		</removeAbandonedTimeout>
		<maxWait>1000</maxWait>
		<timeBetweenEvictionRunsMillis>10000
		</timeBetweenEvictionRunsMillis>
		<numTestsPerEvictionRun>10
		</numTestsPerEvictionRun>
		<minEvictableIdleTimeMillis>10000
		</minEvictableIdleTimeMillis>
	</db-connection-pool>

	<!-- user db cluster -->
	<cluster name="user" catalog="mysql">
