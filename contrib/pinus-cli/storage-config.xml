			<master>
				<db.username>itings</db.username>
				<db.password>itingspwd</db.password>
				<db.url>jdbc:mysql://192.168.4.138:3306/user?useUnicode=true&amp;characterEncoding=UTF-8&amp;zeroDateTimeBehavior=convertToNull&amp;transformedBitIsBoolean=true</db.url>
			</master>
		</global>
		<region capacity="1-999999999999999">
